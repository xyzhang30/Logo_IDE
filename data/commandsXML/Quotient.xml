<?xml version="1.0" encoding="UTF-8" ?>
<command>
  <canonical_name>product</canonical_name>
  <category>Math Command</category>
  <description>Returns quotient of the values of expr1 and expr2</description>
  <example>quotient 10 2</example>
  <parameters>
    <expr1>double, the dividend</expr1>
    <expr2>double, the divisor</expr2>
  </parameters>
  <return_value>Double, result of expr1/expr2</return_value>
  <number_of_expected_parameters>2</number_of_expected_parameters>
  <implementing_class>mathcommand/QuotientCommand</implementing_class>
</command>