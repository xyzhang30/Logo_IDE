<?xml version="1.0" encoding="UTF-8" ?>
<command>
  <canonical_name>or</canonical_name>
  <category>Boolean Command</category>
  <description>Returns 1 if test1 or test2 are non-zero, otherwise 0</description>
  <example>OR 1 0</example>
  <parameters>
    <test1>first number</test1>
    <test2>second number</test2>
  </parameters>
  <return_value>Double, 1 if at least one of test1 and test2 are non-zero, 0 otherwise</return_value>
  <number_of_expected_parameters>2</number_of_expected_parameters>
  <implementing_class>mathcommand/OrCommand</implementing_class>
</command>