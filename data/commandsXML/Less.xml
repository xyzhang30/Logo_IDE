<?xml version="1.0" encoding="UTF-8" ?>
<command>
  <canonical_name>less</canonical_name>
  <category>Boolean Command</category>
  <description>returns 1 if the value of expr1 is strictly less than the value of expr2, otherwise 0</description>
  <example>LESS? 1 1.001</example>
  <parameters>
    <expr1>first number to compare</expr1>
    <expr2>second number to compare</expr2>
  </parameters>
  <return_value>Double, 1 if expr1 is less than expr2, 0 otherwise</return_value>
  <number_of_expected_parameters>2</number_of_expected_parameters>
  <implementing_class>mathcommand.LessCommand</implementing_class>
</command>